    <sheets>
        <sDEF>
            <ROOT>
                <TCEforms>
                    <sheetTitle>Einstellungen </sheetTitle>
                </TCEforms>
                <type>array</type>
                <el>
                    <settings.position>
                        <TCEforms>
                            <label>Position (zb. sticky, absolut, relativ)</label>
                            <config>
                                <type>select</type>
                                <renderType>selectSingle</renderType>
                                <items type="array">
                                    <numIndex index="0" type="array">
                                        <numIndex index="0">Standard</numIndex>
                                        <numIndex index="1"></numIndex>
                                    </numIndex>
                                    <numIndex index="1" type="array">
                                        <numIndex index="0">Sticky</numIndex>
                                        <numIndex index="1">sticky</numIndex>
                                    </numIndex>
                                    <numIndex index="2" type="array">
                                        <numIndex index="0">Absolut</numIndex>
                                        <numIndex index="1">absolute</numIndex>
                                    </numIndex>
                                    <numIndex index="3" type="array">
                                        <numIndex index="0">Relativ</numIndex>
                                        <numIndex index="1">relative</numIndex>
                                    </numIndex>
                                    <numIndex index="4" type="array">
                                        <numIndex index="0">Fixiert</numIndex>
                                        <numIndex index="1">fixed</numIndex>
                                    </numIndex>
                                </items>
                            </config>
                        </TCEforms>
                    </settings.position>
                    <settings.contentFrameWidth>
                        <TCEforms>
                            <label>Inhaltsbreite</label>
                            <config>
                                <type>select</type>
                                <renderType>selectSingle</renderType>
                                <items type="array">
                                    <numIndex index="0" type="array">
                                        <numIndex index="0">Standard</numIndex>
                                        <numIndex index="1"></numIndex>
                                    </numIndex>
                                    <numIndex index="1" type="array">
                                        <numIndex index="0">Volle Breite</numIndex>
                                        <numIndex index="1">full</numIndex>
                                    </numIndex>
                                </items>
                            </config>
                        </TCEforms>
                    </settings.contentFrameWidth>

                    <settings.paddingVertical>
                        <TCEforms>
                            <label>Innenabstamd vertikal</label>
                            <config>
                                <type>select</type>
                                <renderType>selectSingle</renderType>
                                <itemsProcFunc>Atomicplan\PlateLayoutCes\Utility\FlexTCADefaults->bsPyClasses</itemsProcFunc>
                            </config>
                        </TCEforms>
                    </settings.paddingVertical>
                </el>
            </ROOT>
