<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<T3DataStructure>
    <meta>
        <langDisable>1</langDisable>
    </meta>
    <sheets>
        <sDEF>
            <ROOT>
                <TCEforms>
                    <sheetTitle>Einstellungen</sheetTitle>
                </TCEforms>
                <type>array</type>
                <el>
                    <settings.logoWidth>
                        <TCEforms>
                            <label>Logo Breite in Pixeln</label>
                            <config>
                                <type>input</type>>
                            </config>
                        </TCEforms>
                    </settings.logoWidth>
                    <settings.isSticky>
                        <TCEforms>
                            <label>Header Sticky</label>
                            <config>
                                <type>check</type>
                            </config>
                        </TCEforms>
                    </settings.isSticky>

                    <settings.mainNavId>
                        <TCEforms>
                            <label>Id Haupt-Navigation</label>
                            <config>
                                <type>input</type>>
                            </config>
                        </TCEforms>
                    </settings.mainNavId>
                    <settings.topNavId>
                        <TCEforms>
                            <label>Id Top-Navigation</label>
                            <config>
                                <type>input</type>>
                            </config>
                        </TCEforms>
                    </settings.topNavId>
                    <settings.smallNavGap>
                        <TCEforms>
                            <label>Kleine Navi - Abstände</label>
                            <config>
                                <type>select</type>
                                <renderType>selectSingle</renderType>
                                <items type="array">
                                    <numIndex index="0" type="array">
                                        <numIndex index="0">Standard</numIndex>
                                        <numIndex index="1"></numIndex>
                                    </numIndex>
                                    <numIndex index="1" type="array">
                                        <numIndex index="0">Sehr klein</numIndex>
                                        <numIndex index="1">a-gap::extra-small</numIndex>
                                    </numIndex>
                                    <numIndex index="2" type="array">
                                        <numIndex index="0">Klein</numIndex>
                                        <numIndex index="1">a-gap::small</numIndex>
                                    </numIndex>
                                    <numIndex index="3" type="array">
                                        <numIndex index="0">Medium</numIndex>
                                        <numIndex index="1">a-gap::medium</numIndex>
                                    </numIndex>
                                    <numIndex index="4" type="array">
                                        <numIndex index="0">Groß</numIndex>
                                        <numIndex index="1">a-gap::big</numIndex>
                                    </numIndex>
                                    <numIndex index="5" type="array">
                                        <numIndex index="0">Sehr groß</numIndex>
                                        <numIndex index="1">a-gap::large</numIndex>
                                    </numIndex>
                                    <numIndex index="6" type="array">
                                        <numIndex index="0">Riesig</numIndex>
                                        <numIndex index="1">a-gap::huge</numIndex>
                                    </numIndex>
                                </items>
                            </config>
                        </TCEforms>
                    </settings.smallNavGap>
                </el>
            </ROOT>
        </sDEF>
    </sheets>
</T3DataStructure>
